*[hidden]
	display none!important
.public-input
	appearance none
	border-radius 5px
	border 1px solid #999898
	font-size 28px
	color #000
	line-height 28px
	padding 20px
	box-sizing border-box
	outline none
	&.box-shadow
		width 100%
		box-shadow 1px 1px 3px #ccc,
		inset 1px 1px 3px #ccc
	&.submit
		margin-top 70px
		width 100%
		color #fff
		line-height 80px
		padding 0
		background #8c8a8a
.main
	.login
		margin 0 30px
		border-top 1px solid #000
		.quick-login
			padding 80px 0 50px
			display -webkit-flex
			display -moz-flex
			display -ms-flex
			display -o-flex
			display flex
			justify-content center
			a
				display block
				margin 0 20px
				height 70px
				width 260px
				line-height 70px
				text-align center
				font-size 24px
				color #fff
				border-radius 5px
			.facebooks
				background #546ab5
			.line
				background #14b416
		.or-you-can
			font-size 28px
			color #363535
			text-align center
			padding-bottom 40px
		.form
			margin 0 90px 100px
			padding 36px 50px 110px
			background url(../img/left-top.png) left top/54px 54px no-repeat,
			url(../img/right-top.png) right top/54px 54px no-repeat,
			url(../img/left-bottom.png) left bottom/54px 54px no-repeat,
			url(../img/right-bottom.png) right bottom/54px 54px no-repeat;
			label
				display block
			.public-input
				&.input-code
					width 250px
			.name
				display block
				padding 30px 0 16px
				font-size 28px
				color #363535
			.rand-code
				display -webkit-flex
				display -moz-flex
				display -ms-flex
				display -o-flex
				display flex
				align-items center
				justify-content flex-start
				.input
					width 254px
				.code
					margin 0 30px 0 40px
					height 78px
					display block
				.refresh
					display block
					img
						display block
						width 54px
			.btn-option
				padding-top 30px
				font-size 28px
				color #000
				text-align center
				span
					padding 0 40px
				a
					color #546ab5
	.order
		.option
			padding 0 30px
			display -webkit-flex
			display -moz-flex
			display -ms-flex
			display -o-flex
			display flex
			align-items center
			justify-content space-around
			font-size 30px
			background rgba(64,63,63,0.5)
			color #fff
			line-height 150px
			background #9a9a9at
			border-top 1px solid #000
			a
				display block
				&:hover
				&.active
					color #403f3f
		.list
			padding 0 145px
			.item
				padding-bottom 50px
				font-size 30px
				color #363535
				line-height 1.5
				.id
					padding 70px 0 30px
				.row
					display -webkit-flex
					display -moz-flex
					display -ms-flex
					display -o-flex
					display flex
					span
						display block
					.w5em
						width 5em
		.base-info
			padding 70px 145px 100px
			.row
				padding-bottom 30px
				label
					display -webkit-flex
					display -moz-flex
					display -ms-flex
					display -o-flex
					display flex
					align-items center
					justify-content flex-start
					span
						display block
						font-size 30px
						color #363535
						width 100px
					input
						width calc(100% - 100px)
				.select
					padding-right 40px
					background  url(../img/arrow-bottom.png) right 10px center/26px 22px no-repeat
					&+.select
						margin-left 30px
		.change-password
			.row
				label
					span
						width 200px
					input
						width calc(100% - 200px)
	.shoping-car
		.option
			padding 0 30px
			display -webkit-flex
			display -moz-flex
			display -ms-flex
			display -o-flex
			display flex
			align-items center
			justify-content space-around
			font-size 30px
			background url(../img/arrow-right-white.png) 33% center/36px 23px no-repeat,
			rgba(64,63,63,0.5) url(../img/arrow-right-white.png) 66% center/36px 23px no-repeat
			color #fff
			line-height 150px
			background #9a9a9at
			border-top 1px solid #000
			a
				display block
				&:hover
				&.active
					color #403f3f
		.product-list
			margin 30px
			border-top 1px solid #000
			.item
				display -webkit-flex
				display -moz-flex
				display -ms-flex
				display -o-flex
				display flex
				justify-content space-between
				padding-bottom 30px
				border-bottom 1px solid #000
				.pic
					flex 0 0 150px
					img
						width 150px
				.text
					display -webkit-flex
					display -moz-flex
					display -ms-flex
					display -o-flex
					display flex
					flex-direction column
					justify-content space-between
					flex 1 1 auto
					padding 0 20px
					font-size 24px
					color #363535
					line-height 1
					h3
						padding 20px 0
					.size
						padding 0 0 20px
					.row
						display -webkit-flex
						display -moz-flex
						display -ms-flex
						display -o-flex
						display flex
						justify-content space-between
						align-items flex-end
						.cost
							color #d00d0d
						.number
							.name
								line-height 40px
								float left
							.wrap
								margin-left 10px
								float left
								border 1px solid #acacac
								line-height 38px
								input
									float left
									border none
									outline none
									width 60px
									height 38px
									text-align center
								span
									float left
									height 38px
									width 40px
									text-align center
		.total
			padding-bottom 70px
			margin 0 30px
			text-align right
			font-size 24px
			color #363535
			span
				color #d00d0d
				padding 0 5px
		.pay-mode
			margin 0 30px
			h3
				padding 10px 0
				font-size 24px
				color #363535
				text-align center
				border-bottom 1px solid #313131
			.row
				padding 30px 0 60px
				width 80%
				margin 0 auto
				display -webkit-flex
				display -moz-flex
				display -ms-flex
				display -o-flex
				display flex
				justify-content space-around
				font-size 24px
				color #363535
				.item
					position relative
					width 135px
					height 135px
					display -webkit-flex
					display -moz-flex
					display -ms-flex
					display -o-flex
					display flex
					align-items center
					justify-content center
					border 1px solid #acacac
					&.active
						&:after
							content ""
							position absolute
							width 62px
							height 51px
							bottom 0
							right -18px
							background url(../img/right.png) center center/100% 100% no-repeat
		.pay
			padding-bottom 100px
			a
				display block
				width 372px
				height 54px
				margin 0 auto
				text-align center
				font-size 24px
				color #fff
				padding-left 3px
				letter-spacing 3px
				box-sizing border-box
				line-height 54px
				background #9a9a9a url(../img/arrow-right-white-round.png) right 10px center /25px 25px no-repeat
	.shoping-car-step-two
		padding 0 30px
		font-size 24px
		color #363535
		.public-title
			padding 10px 0
			color #363535
			text-align center
			border-bottom 1px solid #313131
		.sub-title
			padding 40px 0 20px
		.public-input
			font-size 24px
			padding 10px
			margin 5px 0
			width 100%
			height 40px
			line-height 40px
			box-sizing content-box
			// height 50Px
		.content
			padding 0 50px 70px
		// .pay-information
		.row
			display -webkit-flex
			display -moz-flex
			display -ms-flex
			display -o-flex
			display flex
			align-items center
			justify-content space-between
			// display block
		.column-4
			input
				flex 0 0 19%
				width 19%
				display block
		.w50
			justify-content space-between
			.item
				flex 0 0 48%
			select
				display block
				flex 0 0 40%
				width 40%
		select
			background url(../img/arrow-bottom.png) right 10px center/24px 20px no-repeat
		.invoice-data
			.btn-group
				display -webkit-flex
				display -moz-flex
				display -ms-flex
				display -o-flex
				display flex
				padding 50px 0 30px
				a
					display block
					width 150px
					height 50px
					line-height 50px
					text-align center
					border 1px solid #636262
					&:hover,
					&.active
						background #363535
						color #fff
						border-color #707070
					&+a
						margin-left 30px
			.wrap
				.row
					justify-content flex-start
					align-items center
					select
						width 200px
					label
						display block
						margin-left 20px
					input[type=checkbox]
						margin-right 10px
						width 32px
						height 32px
						border 1px solid #999898
						appearance none
						outline none
						vertical-align middle
						&:checked
							background url(../img/right-small.png) center center/24px 24px no-repeat
					span
						vertical-align middle
		.submit
			.row
				justify-content space-between
			.pre
				width 210px
				height 55px
				font-size 24px
				line-height 55px
				padding-left 30px
				text-align center
				color #fff
				background #9a9a9a url(../img/arrow-left-white-round.png) 10px center /25px 25px no-repeat
			.help
				width 80px
				height 55px
				background url(../img/help.png) center center/80px 49px no-repeat
			.next
				width 370px
				height 55px
				line-height 55px
				text-align center
				padding-right 30px
				font-size 24px
				color #fff
				background #9a9a9a url(../img/arrow-right-white-round.png) right 10px center /25px 25px no-repeat
	.success
		padding 170px 0
		text-align center
		color #363535
		line-height 2
	.product-details
		padding 0 60px
		.base-info
			.pic
				img
					display block
					width 100%
			.content
				h3
					font-size 36px
					color #363535
					line-height 2
				p
					font-size 36px
					color #363535
					&.id
						padding-top 80px
					span
						text-decoration line-through
						&+span
							padding-left 95px
						&.red
							text-decoration none
							color #f0053c
		.size
			padding 40px 0
			select
				width 300px
				text-align center
				background url(../img/drop-down.png) right 10px center/19px 17px no-repeat
		.btn-group
			padding 0 30px
			a
				display block
				font-size 28px
				color #fff
				width 274px
				height 70px
				line-height 70px
				text-align center
				background #fb4876
		.row
			display -webkit-flex
			display -moz-flex
			display -ms-flex
			display -o-flex
			display flex
			align-items center
			justify-content space-between
		.product-info-area
			padding 10px 0
			img
				display block
				width 100%
	.page-control
		margin-bottom 60px
		padding 0 60px
		display -webkit-flex
		display -moz-flex
		display -ms-flex
		display -o-flex
		display flex
		align-items center
		justify-content flex-end
		font-size 20px
		color #000
		line-height 50px
		.total
			padding-right 10px
